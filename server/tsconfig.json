{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "allowJs": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "removeComments": true,
    "newLine": "LF",
    "pretty": true,
    "noFallthroughCasesInSwitch":true,
    "forceConsistentCasingInFileNames":true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "lib": [
      "es2017",
    ],
    "noUnusedLocals": true,
    "noUnusedParameters": true
  },
}